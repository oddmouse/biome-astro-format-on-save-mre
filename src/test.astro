---
const useSortedKeys = {
	2: "two",
	3: "three",
	1: "one",
	4: "four",
};

let useConst = "Astro Test";
---

<!doctype html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <title>{useConst}</title>
  </head>
  <body>
    <h1>{useConst}</h1>
    <ol>
      {Object.values(useSortedKeys).map((val) => <li>{val}</li>)}
    </ol>
  </body>
</html>
